                <div class="col-3 bm-2">
                    <p>Employee Name:</p>
                </div>
                <div class="col-3 bm-2" t-if="o.employee_id.name" style="font-size:10px">
                    <p><span t-field="o.employee_id.name"/></p>
                </div>
                <div class="col-3 bm-2" style="font-size:10px">
                    <p>Month:</p>
                </div>
                <div class="col-3 bm-2" t-if="o.name" style="font-size:10px">
                    <p><span t-esc="month"/></p>
                </div>
            </div>
            <div class='row'>
                <div class="col-3 bm-2" style="font-size:10px">
                    <p>Employee Code:</p>
                </div>
                <div class="col-3 bm-2" t-if="o.employee_id.registration_number" style="font-size:10px">
                    <p><span t-field="o.employee_id.registration_number"/></p>
                </div>
                <div class="col-3 bm-2" style="font-size:10px">
                    <p>Bank Account:</p>
                </div>
                <div class="col-3 bm-2" t-if="o.employee_id.bank_account_id.bank_id.name" style="font-size:10px">
                    <p><span t-field="o.employee_id.bank_account_id.bank_id.name"/></p>
                </div>
            </div>
            <div class='row'>
                <div class="col-3 bm-2" style="font-size:10px">
                    <p>Level:</p>
                </div>
                <div class="col-3 bm-2" t-if="o.employee_id.level_id.code" style="font-size:10px">
                    <p><span t-field="o.employee_id.level_id.code"/></p>
                </div>
                <div class="col-3 bm-2" style="font-size:10px">
                    <p>Currency:</p>
                </div>
                <div class="col-3 bm-2" t-if="o.employee_id.bank_account_id.company_id.currency_id.name" style="font-size:10px">
                    <p><span t-field="o.employee_id.bank_account_id.company_id.currency_id.name"/></p>
                </div>
            </div>
